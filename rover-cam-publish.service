[Unit]
Description=Rover Camera LiveKit Publisher
After=network.target rover-cam-gstreamer.service
Requires=rover-cam-gstreamer.service

[Service]
ExecStart=/bin/bash /home/dc/rover-teleop/rover/cam_publish.sh
Restart=on-failure
User=dc
EnvironmentFile=/home/dc/rover-teleop/rover/.env

[Install]
WantedBy=multi-user.target